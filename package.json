{
  "name": "tailwind-for-wordpress",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules/.cache && rm -rf dist/",
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "yarn run clean && npm run production",
    "production": "yarn run clean && cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.11.5",
    "cross-env": "^5.2.0",
    "jquery": "^3.5.1",
    "laravel-mix": "^5.0.9",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-jsx": "^3.3.0",
    "tailwindcss": "^1.0.3",
    "vue": "^2.6.12",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@styled-jsx/plugin-sass": "^2.0.1",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/babel-plugin-transform-vue-jsx": "^1.1.2",
    "babel-plugin-jsx-vue-functional": "^2.1.0",
    "browser-sync": "^2.26.12",
    "browser-sync-webpack-plugin": "^2.0.1",
    "hard-source-webpack-plugin": "^0.13.1",
    "laravel-mix-make-file-hash": "^2.1.0",
    "laravel-mix-polyfill": "^2.0.0",
    "postcss-custom-properties": "^10.0.0",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "targets-webpack-plugin": "^3.0.0",
    "vue-template-compiler": "^2.6.12"
  }
}
